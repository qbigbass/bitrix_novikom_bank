---
import DefaultPageLayout from "../layouts/DefaultPageLayout.astro";
import SectionSeeAlso from "../components/sections/SectionSeeAlso.astro";
import SectionCallMeBack from "../components/sections/SectionCallMeBack.astro";
import TextBanner from "../components/banners/TextBanner.astro";
import SectionShowcaseCardsCatalog from "../components/sections/SectionShowcaseCardsCatalog.astro";

const breadcrumbs = [{
	title: 'Частным клиентам',
	url: '#'
}]
---
<DefaultPageLayout title="Банковские карты">
	<TextBanner
		title="Банковские карты"
		breadcrumbs={breadcrumbs}
		description="Карты с оптимальными условиями и приятными бонусами"
	/>
	<SectionShowcaseCardsCatalog />
	<SectionSeeAlso />
	<SectionCallMeBack />
</DefaultPageLayout>