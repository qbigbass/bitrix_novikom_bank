---
import DefaultPageLayout from "../layouts/DefaultPageLayout.astro";
import SectionSeeAlso from "../components/sections/SectionSeeAlso.astro";
import SectionCallMeBack from "../components/sections/SectionCallMeBack.astro";
import TextBanner from "../components/banners/TextBanner.astro";
import SectionMortgageProgramsCatalog from "../components/sections/SectionMortgageProgramsCatalog.astro";

const breadcrumbs = [{
	title: 'Частным клиентам',
	url: '#'
}]
---
<DefaultPageLayout title="Ипотечные программы">
	<TextBanner
		title="Ипотечные программы"
		breadcrumbs={breadcrumbs}
		description="Быстрые кредиты по оптимальной ставке на любые цели"
        borderColor="green"
	/>
	<SectionMortgageProgramsCatalog />
	<SectionSeeAlso />
	<SectionCallMeBack />
</DefaultPageLayout>
