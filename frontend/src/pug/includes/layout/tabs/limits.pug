-
    var tableData = [
        {
            label: "Сумма одного перевода",
            value: ["От&nbsp;100 до&nbsp;70&nbsp;000&nbsp;рублей, без учёта дополнительной комиссии"]
        },
        {
            label: "Максимальная сумма переводов в&nbsp;сутки",
            value: ["300&nbsp;000 рублей с&nbsp;учётом дополнительной комиссии по&nbsp;одной карте"]
        },
        {
            label: "Максимальная сумма переводов за&nbsp;календарный месяц",
            value: ["1&nbsp;500&nbsp;000&nbsp;рублей, с&nbsp;учётом дополнительной комиссии по&nbsp;одной карте"]
        },
        {
            label: "Максимальное количество переводов в&nbsp;сутки",
            value: ["8&nbsp;переводов по&nbsp;одной карте"]
        },
        {
            label: "Максимальное количество переводов за&nbsp;4&nbsp;календарных дня",
            value: ["20&nbsp;переводов по&nbsp;одной карте"]
        },
    ]

.table-tab.cell-2
    .table-tab__body
        each row in tableData
            .table-tab__row
                .table-tab__cell.text-l.fw-semibold.dark-70!= row.label
                .table-tab__cell
                    each item in row.value
                        p.text-l!= item
